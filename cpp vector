#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<int>arr={5,4,3,2,1};
    arr.push_back(5);//apends an element 
    arr.emplace_back(5);//apends an element 
    auto x = arr.begin();//gives the first index address
    auto y = arr.end()-1;//gives the last index address
    cout<<*x<<endl;//* indicates element in that specific address
    
    int summ=0;
    for (auto i=arr.begin();i<arr.end();i++){
            summ+=*i;
    }
    sort(arr.begin(),arr.end());
    for (auto i=arr.begin();i<arr.end();i++){
        cout<<*i<<" "<<endl;
    }
    
    
    cout<<summ;
    }
    
    
    
